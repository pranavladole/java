package example;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class Test {

	public static void main(String[] args) {
		
		List<Integer> list = Arrays.asList(5,10,12,20,21);
		List<Integer> filteredlist =  list.stream().filter(t -> t%2==0).toList();		
		System.out.print(filteredlist);
		
	}

}
