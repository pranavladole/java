				count+=1;
			}
		}
		
		return count;

// to check if it is sqrt of 2 
public static boolean isPowerofTwo(int n) {
    
    boolean check=false;
    for(int i=0;i<n;i++){
            if ( (int)Math.pow(2,i) == n  ){
             check =  true;
             break;
            }else{
                check= false;
            }
          }
          return check;
    }

